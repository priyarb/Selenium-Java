package JavaPractice;

public class StringReverse {

	public static void main(String[] args) {

		String s="Selenium";
		String rev="";
		int len=s.length();
		for (int i=len-1;i>=0;i--) {
			rev=rev+s.charAt(i);
		}
		System.out.println(rev);
		
		//String Buffer
		String s1="java";
		StringBuffer sf=new StringBuffer(s1);
		System.out.println(sf.reverse());
	}

}
