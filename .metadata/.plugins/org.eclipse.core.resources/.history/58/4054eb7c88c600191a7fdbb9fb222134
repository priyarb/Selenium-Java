package JavaPractice;

public class PrimeNumber {
	//number divisible by 1 or itself
	//13
	//13/2: 13/3: 13/4: 13/5......
	
	public static boolean isPrimeNumber(int num) {
		//edge/corner condition
		while(num<=1) {
			return false;
		}
		
		for(int i=2;i<num;i++) {
			if(num%i==0)
				return false;
		}
		return true;
	}
	

	public static void main(String[] args) {


		
	}

}
